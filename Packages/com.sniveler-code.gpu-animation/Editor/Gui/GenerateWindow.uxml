            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="PrefabsContent" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: column; align-items: stretch; justify-content: flex-start; display: flex; align-self: stretch;">
            <ui:VisualElement name="PrefabList" style="flex-grow: initial; background-color: rgba(0, 0, 0, 0); height: auto; padding-left: 5px; align-items: flex-end; width: 100%;" />
        </ui:VisualElement>
    </ui:VisualElement>
