        <ui:VisualElement style="flex-grow: 0; background-color: rgba(0, 0, 0, 0); flex-direction: row; height: auto; flex-shrink: 0; align-items: stretch;">
            <ui:TextField picking-mode="Ignore" label="Batch Models Name" name="BatchName" style="margin-top: 3px; margin-bottom: 3px; height: 20px; flex-grow: 1;" />
            <ui:VisualElement name="BatchActions" style="flex-grow: 0; background-color: rgba(0, 0, 0, 0); flex-direction: row;">
                <ui:Button text="Add Prefab" display-tooltip-when-elided="true" enable-rich-text="false" name="AddPrefab" tooltip="Add prefab to bake" style="display: flex; visibility: visible; margin-top: 3px; margin-bottom: 3px;" />
